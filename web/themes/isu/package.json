{
  "name": "build",
  "version": "0.1.0",
  "scripts": {
    "process": "postcss src/css --dir dist/css && cp src/html/index.html dist/index.html",
    "dev": "npm run process && concurrently 'npm-watch' 'webpack serve --mode development'",
    "build": "webpack --mode production",
    "svgo": "svgo src/svg",
    "stylelint": "stylelint \"**/*.css\"",
    "eslint": "eslint \"**/*.{js,jsx}\"",
    "deploy": "webpack --mode production && zip -r dist/dist.zip dist && s3-deploy \"dist/**/*\" --cwd dist --region us-west-2 --bucket isu.mstoner.com"
  },
  "dependencies": {
    "core-js": "^3.23.4",
    "flickity": "^3.0.0",
    "flickity-imagesloaded": "^2.0.0",
    "focus-trap": "^7.0.0",
    "focus-visible": "^5.2.0",
    "focus-within": "^3.0.2",
    "html-react-parser": "^3.0.4",
    "minimodal": "^1.2.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.11.3",
    "regenerator-runtime": "^0.13.9",
    "s3-deploy": "^1.4.0",
    "swiper": "^8.4.5",
    "url-search-params-polyfill": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.1.0",
    "postcss-hexrgba": "^2.1.0",
    "postcss-import": "^14.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-root-var": "^0.1.4",
    "slugify": "^1.6.5",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "svgo": "^2.8.0",
    "tailwindcss": "^3.2.4",
    "twig-html-loader": "^0.1.9",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
