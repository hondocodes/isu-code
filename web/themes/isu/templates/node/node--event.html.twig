{% if view_mode == 'headline' %}
  <a href="{{ url }}" class="event flow bg-blue invert py-l px-m">
    <div class="event__date h3">
      {% if end_date %}
        {{ start_date }} - {{ end_date}}
      {% else %}
        {{ start_date }}
      {% endif %}
    </div>
    <h3 class="event__title font-base text-step-0">
      {{ label }}
    </h3>
    <div class="event__details">
      {% if not end_date %}
        <div class="event flex items-center gap-xs">
          <span class="material-symbols-outlined" aria-hidden="true">
            schedule
          </span>
          {{ content.field_event_date }}
        </div>
      {% endif %}
      {% if content.field_contact_venue|render %}
        <div class="flex items-center gap-xs">
          <span class="material-symbols-outlined" aria-hidden="true">
            location_on
          </span>
          {{ content.field_contact_venue }}
        </div>
      {% endif %}
    </div>
  </a>
{% elseif view_mode == 'headline_alt' %}
  <a href="{{ url }}" class="event flow bg-blue-light py-l px-m">
    <div class="event__date h3">
      {% if end_date %}
        {{ start_date }} - {{ end_date}}
      {% else %}
        {{ start_date }}
      {% endif %}
    </div>
    <h3 class="event__title font-base text-step-0">
      {{ label }}
    </h3>
    <div class="event__details">
      <div class="event flex items-center gap-xs">
        <span class="material-symbols-outlined" aria-hidden="true">
          schedule
        </span>
        {{ content.field_event_date }}
      </div>
      {% if content.field_contact_venue|render %}
        <div class="flex items-center gap-xs">
          <span class="material-symbols-outlined" aria-hidden="true">
            location_on
          </span>
          {{ content.field_contact_venue }}
        </div>
      {% endif %}
    </div>
  </a>
{% elseif view_mode == 'announcement' %}
  <a href="{{ url }}" class="event event--alt invert">
    {% if end_month %}
    <div class="event__date event__date--range">
    {% else %}
    <div class="event__date">
    {% endif %}
      {% if not end_month %}
        {{ start_month }} <span>{{ start_day }}</span>
      {% else %}
        {{ start_month }} {{ start_day }}
      {% endif %}
      {% if end_month %}
        <span>&mdash;</span>
        {{ end_month }} {{ end_day }}
      {% endif %}
    </div>
    <div class="event__details">
      <h3 class="event__title font-base text-step-0">
        {{ label }}
      </h3>
    </div>
  </a>
{% else %}
<div class="wrapper">

<div class="flow relative">


<div class="flow flow-space-l">

<div class="flow flow-space-l bordered">



<div class="feature feature--xs event__singular" data-direction="rtl" data-animation="fade-in-up" data-animate="true"><div class="frame" data-ratio="3:2">


{{content.field_event_image}}

</div>



<div class="flow flow-space-s">
<div id="event_tag__container">
<h4>Description</h4>
{{content.field_event_description}}
<span class="event__tags">Primary Topic:  {{content.field_event_primary_topic}} Tags:  {{content.field_event_tags}}</span>
</div>

<p><span class="event__info">Date(s):  </span>{{content.field_event_date}}</p>
<p><span class="event__info">Venue:  </span>{{content.field_event_venue}}</p>
<h4>Contact Info</h4>
<p><span class="event__info">Name:  </span>{{content.field_event_contact_name}}</p>
<p><span class="event__info">Email:  </span>{{content.field_event_contact_email}}</p>
<p><span class="event__info">Phone:  </span>{{content.field_event_contact_phone}}</p>


</div>


</div>






</div>

</div>

</div>


</div>
{% endif %}
